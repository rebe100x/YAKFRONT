!!!
html
	head
		meta(charset='utf8')
		base(href='/')
		title Yakwala Backend - #{JSON.parse(conf).env} - #{JSON.parse(mainConf).versionback}
		meta(name="viewport", content="width=device-width, initial-scale=1.0") 
		link(rel="shortcut icon",href="/images/yakfav.png", type="image/x-icon")
		- if(JSON.parse(conf).env == 'DEV')
			link(rel='stylesheet', href='/javascripts/lib/bootstrap/css/bootstrap.css')
			link(rel='stylesheet', href='/javascripts/lib/bootstrap/css/bootstrap-responsive.css')
			link(rel='stylesheet', href='/javascripts/lib/jquery/css/custom-theme/jquery-ui-1.8.24.custom.css')
			link(rel='stylesheet', href='/javascripts/lib/jquery/css/jquery.mCustomScrollbar.css')
			link(rel='stylesheet', href='/javascripts/lib/fuelux/dist/css/fuelux.css')
			link(rel='stylesheet', href='/javascripts/lib/toastr/toastr.css')
			//link(rel='stylesheet', href='/javascripts/lib/jquery/css/imgareaselect-default.css')
			link(rel='stylesheet', href='/stylesheets/common.css?v='+JSON.parse(mainConf).versionback)
			link(rel='stylesheet', href='/stylesheets/backstyle.css?v='+JSON.parse(mainConf).versionback)
		- else
			link(rel='stylesheet', href='/stylesheets/minify/back-min-'+JSON.parse(mainConf).versionback+'.css')

		- if(JSON.parse(conf).env == 'DEV')		
			script(src='/javascripts/lib/jquery/js/jquery-1.8.2.min.js')
			script(src='/javascripts/lib/jquery/js/jquery-ui-1.8.24.custom.min.js')
			script(src='https://maps.googleapis.com/maps/api/js?v=3.9&sensor=false&libraries=drawing,places&key='+JSON.parse(conf).gmapKey)		
			script(src='https://www.google.com/jsapi')
			script(src='/javascripts/lib/plugin/jquery.form.js')
			script(src='/javascripts/lib/toastr/toastr.js')
			script(src="/javascripts/lib/jquery/js/timeago.js")
			script(src='/javascripts/lib/plugin/happy.js')
			script(src='/javascripts/lib/jquery/js/jquery.ui.widget.js')
			//script(src='/javascripts/lib/jquery/js/jquery.imgareaselect.pack.js')
			script(src='/javascripts/lib/jquery/js/jquery.iframe-transport.js')	
			script(src='/javascripts/lib/jquery/js/jquery.fileupload.js')
			script(src='/javascripts/lib/fuelux/lib/require.js')
			script(src='/javascripts/lib/fuelux/dist/loader.min.js')
			script(src='/javascripts/lib/bootstrap/js/bootstrap.min.js')
			script(src='/javascripts/lib/bootstrap/js/bootstrap-typeahead.js')		
			script(src='/javascripts/lib/plugin/jquery.cookie.js')
			script(src='/javascripts/lib/plugin/jquery.mousewheel.min.js')
			script(src='/javascripts/lib/plugin/jquery.mCustomScrollbar.min.js')
			script(src="/javascripts/lib/jquery/js/jquery.md5.min.js")
			script(src='/javascripts/lib/plugin/string.min.js')
			script(src='/javascripts/lib/bootstrap/js/bootstrap-contextmenu.js')

			script(src='/javascripts/common.js?v='+JSON.parse(mainConf).versionback)
			script(src='/javascripts/back_common.js?v='+JSON.parse(mainConf).versionback))
		- else
			script(src='https://maps.googleapis.com/maps/api/js?v=3.9&sensor=false&libraries=drawing&key='+JSON.parse(conf).gmapKey)		
			script(src='/javascripts/minify/back-min-'+JSON.parse(mainConf).versionback+'.js')

		
		block head
	body
		block content
		
