mixin list()
	div.span6.raphael
		div#chart_div(style="width: 900px; height: 500px;")	

	div.span6 elo
				
	script
		window.onload = function () {
			
			
			
		};
		var last10Days = new Date() - 10*24*60*60*1000;
			
		google.load("visualization", "1", {packages:["corechart"]});
		google.setOnLoadCallback(drawChart);
		function drawChart() {
			
			
			//console.log(data);	
			$.get('/api/dashboard/stats/'+last10Days,function(data){
				console.log(data.stats);
				var data = google.visualization.arrayToDataTable(data.stats);
				var data = google.visualization.arrayToDataTable([
					['Year', 'Sales', 'Expenses'],
					['2004',  1000,      400],
					['2005',  1170,      460],
					['2006',  660,       1120],
					['2007',  1030,      540]
					]);
				var options = {
					title: 'Company Performance',
					hAxis: {title: 'Year',  titleTextStyle: {color: 'red'}, format:'MMM d, y'}
				};

				var chart = new google.visualization.AreaChart(document.getElementById('chart_div'));
				chart.draw(data, options);
			});

			
		}

		