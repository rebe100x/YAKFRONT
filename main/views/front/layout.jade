include mixins/footer.jade
//include mixins/gravatarImagePopup.jade
!!!
html
	head
		meta(charset='utf8')
		base(href='/')
		title Yakwala - #{JSON.parse(conf).env} - #{JSON.parse(mainConf).version}
		meta(name="viewport", content="width=device-width, initial-scale=1.0") 
		link(rel="shortcut icon",href="/images/yakfav.png", type="image/x-icon")
		- if(JSON.parse(conf).env == 'DEV')
			link(rel='stylesheet', href='/javascripts/lib/bootstrap/css/bootstrap.css')
			link(rel='stylesheet', href='/javascripts/lib/bootstrap/css/bootstrap-responsive.css')
			link(rel='stylesheet', href='/javascripts/lib/jquery/css/custom-theme/jquery-ui-1.8.24.custom.css')
			link(rel='stylesheet', href='/stylesheets/common.css?v='+JSON.parse(mainConf).version)
			link(rel='stylesheet', href='/stylesheets/customSB.css')
			link(rel='stylesheet', href='/stylesheets/style.css?v='+JSON.parse(mainConf).version)
		- else
			link(rel='stylesheet', href='/stylesheets/minify/front-min-'+JSON.parse(mainConf).version+'.css')
		

		- if(JSON.parse(conf).env == 'DEV')	
			script(src='/javascripts/lib/jquery/js/jquery-1.8.2.min.js')
			script(src='/javascripts/lib/jquery/js/jquery-ui-1.8.24.custom.min.js')
			script(src='https://maps.googleapis.com/maps/api/js?v=3.9&sensor=false&libraries=drawing&key='+JSON.parse(conf).gmapKey)		
			script(src='/javascripts/lib/bootstrap/js/bootstrap.min.js')
			script(src='/javascripts/lib/bootstrap/js/bootstrap-typeahead.js')
			script(src='/javascripts/lib/plugin/jquery.cookie.js')
			script(src='/javascripts/lib/plugin/jquery.mousewheel.min.js')
			script(src='/javascripts/lib/plugin/happy.js')
			script(src='/javascripts/lib/plugin/jquery.scrollTo-min.js')
			script(src="/javascripts/lib/jquery/js/timeago.js")
			script(src='/javascripts/customSB.js')
			script(src="/javascripts/lib/jquery/js/jquery.md5.min.js")
			script(src='/javascripts/common.js?v='+JSON.parse(mainConf).version)
			script(src='/javascripts/front_common.js?v='+JSON.parse(mainConf).version)

		- else
			script(src='https://maps.googleapis.com/maps/api/js?v=3.9&sensor=false&libraries=drawing&key='+JSON.parse(conf).gmapKey)		
			script(src='/javascripts/minify/front-min-'+JSON.parse(mainConf).version+'.js')

		

		block head
	body	
		block content
		mixin footer()
		//mixin gravatarImagePopup()
		script
			#{JSON.parse(conf).ga}

		
			
			